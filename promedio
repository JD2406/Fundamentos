#include <iostream>

using namespace std;

int main()
{
   float promedio, numero1, numero2;
   
   cout<<"Ingrese el numero 1:  ";
   cin>>numero1;
   
   cout<<"Ingrese el numero 2:  ";
   cin>>numero2;
   
   promedio = numero1 + numero2;
  
   
   cout<<"Ingrese el numero 3:  ";
   cin>>numero1;
   
   cout<<"Ingresa el numero 4:  ";
   cin>>numero2;
   
   promedio = promedio + numero1 + numero2 ;
   
   cout<<"Ingresar el numero 5: ";
   cin>>numero1;
   
   promedio = promedio + numero1 ;
   
   cout<<"El promedio es:  " <<promedio/5;
   
   return 0;
}
